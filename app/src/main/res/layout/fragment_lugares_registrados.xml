<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.lugaresRegistrados.lugaresRegistradosFragment">


    <ImageView
        android:id="@+id/image1Place"
        android:layout_width="211dp"
        android:layout_height="243dp"
        android:src="@drawable/baseline_add_a_photo_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.077" />

    <ImageView
        android:id="@+id/image2Place"
        android:layout_width="126dp"
        android:layout_height="111dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="20dp"
        android:src="@drawable/baseline_add_a_photo_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/image3Place"
        android:layout_width="133dp"
        android:layout_height="112dp"
        android:layout_marginStart="20dp"
        android:src="@drawable/baseline_add_a_photo_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/image1Place"
        app:layout_constraintTop_toBottomOf="@+id/image2Place" />
</androidx.constraintlayout.widget.ConstraintLayout>